{
    "env": {
      "browser": true,
      "es6": true,
      "jest": true
    },
    "extends": ["airbnb-base", "prettier"],
    "plugins": ["prettier"],
    "globals": {
      "Atomics": "readonly",
      "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
      "ecmaVersion": 2018,
      "sourceType": "module"
    },
    "rules": {
      "no-unused-vars": "warn",
      "consistent-return": "off",
      "prettier/prettier": ["error"]
    }
  }
  